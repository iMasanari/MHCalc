{
  "version": "0.1.0",
  "command": "npm",
  "isShellCommand": true,
  "suppressTaskName": true,
  "args": ["run"],
  "tasks": [
    {
      "taskName": "build",
      "problemMatcher": "$tsc",
      "args": ["build"]
    },
    {
      "taskName": "build:watch",
      "args": ["build:watch"],
      "problemMatcher": "$tsc-watch",
      "isBackground": true
    },
    {
      "taskName": "build:prod",
      "problemMatcher": "$tsc",
      "args": ["build:prod"]
    },
    {
      "taskName": "deploy",
      "problemMatcher": "$tsc",
      "args": ["deploy"]
    }
  ]
}