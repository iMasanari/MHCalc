{
    "version": "0.1.0",
    "command": "npm",
    "isShellCommand": true,
    "showOutput": "always",
    "suppressTaskName": true,
    "args": ["run"],
    "tasks": [
        {
            "taskName": "build",
            "args": ["build"],
            "problemMatcher": "$tsc-watch"
        },
        {
            "taskName": "uglify",
            "args": ["uglify"]
        }
    ]
}